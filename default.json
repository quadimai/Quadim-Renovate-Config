{
  "extends": [
    "config:base"
  ],
  "labels": [
    "renovate"
  ],
  "packageRules": [
    {
      "updateTypes": ["minor", "patch", "digest"],
      "matchPackagePatterns": [
        "^ai.quadim.api:quadim", 
        "^github.com/aws/aws-sdk-go",
        "^com.amazonaws:aws-*"
      ],
      "automerge": true,
      "ignoreTests": true
    }
  ]
}
