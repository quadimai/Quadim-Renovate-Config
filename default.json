{
  "extends": [
    "config:base"
  ],
  "labels": [
    "renovate"
  ],
  "automerge": true,
  "major": {
    "automerge": false
  },
  "packageRules": [
    {
      "updateTypes": ["minor", "patch", "digest"],
      "matchPackagePatterns": ["^quadim-typelib", "^quadim-springservicebaselib", "^aws-java-sdk", "^aws-sdk-go"],
      "automerge": true,
      "enabled": true       
    }
  ]
}